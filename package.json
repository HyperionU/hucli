{
  "name": "hyperioncli",
  "version": "1.0.0",
  "description": "The easiest way to configure your development environment to UofH Standards.",
  "type": "module",
  "bin": "./dist/index.js",
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "start": "node dist/index.js",
    "start:help": "node dist/index.js -h",
    "start:version": "node dist/index.js -V",
    "start:nitrox": "node dist/index.js --ntrx",
    "start:turbo": "node dist/index.js --turbo",
    "clean": "rm -rf dist",
    "clean:turbo": "rm -rf dist && pnpm remove -g turbo",
    "quickbuild": "tsc && tsup"
  },
  "keywords": [
    "cli",
    "configure"
  ],
  "author": "HyperionU",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^20.12.11",
    "tsup": "^8.0.2",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "execa": "^9.5.1",
    "gradient-string": "^2.0.2"
  }
}
